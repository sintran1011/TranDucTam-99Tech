{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@icons/*": [
        "src/assets/icons/*"
      ]
    },
    "strict": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "es2015"
    ],
    "composite": true,
  }
}